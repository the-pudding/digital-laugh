<div class='chart-lower'>
	<div class='lower__header'>
		<h4 class='header__title chart-title'>{{{@root.doc.share.chartLowerTitle}}}</h4>
		<ul class='header__key'>
			<li class='key--lol'>lol</li>
			<li class='key--ha'>ha</li>
			<li class='key--acronym'>acronym</li>
			<li class='key--other'>other</li>
			<li class='key--text'>text</li>
		</ul>
	</div>
	
	<figure></figure>
	<p class='lower__note'>Note: {{@root.doc.share.lowerNote}} <span>Show details.</span></p>
	<div class='lower__table'><table class='is-hidden'>
		<thead></thead>
		<tbody></tbody>
	</table></div>
</div>

<div class='prose'>
	{{#each @root.doc.share.tween}}
	<p>{{{value}}}</p>
	{{/each}}
</div>

<div class='chart-case'>
	<div class='case__header'>
		<h4 class='header__title chart-title'>{{{@root.doc.share.chartCaseTitle}}}</h4>
	
		<ul class='header__key'>
			<li><span class='usage'>50%</span><span class='label'>lowercase</span></li>
			<li><span class='label'>Uppercase</span><span class='usage'>50%</span></li>
		</ul>

		<p class='slider-label'>Zoom <span>Level</span></p>

		<div class='header__slider slider'></div>
	</div>

	<figure>
	</figure>	
</div>


