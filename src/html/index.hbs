<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>

	{{> base/meta}}

	<link inline rel='stylesheet' href='bundle.css' />

	{{> base/font}}


	<script inline src='head.js'></script>

	{{> base/analytics }}

</head>

<body>

	{{> base/header 'sticky-dark'}}

	<main id='content'>

		{{> story/intro }}
		{{#each doc.parts}}
			{{> story/part }}
		{{/each}}
		{{> story/outro }}
	</main>

	{{> base/footer }}


	<script src='{{basepath}}assets/scripts/d3.v5.11.0.min.js'></script>
	<script src='main.js?version={{timestamp}}' async></script>
</body>

</html>